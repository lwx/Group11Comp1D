module compare (
    input z,
    input v,
    input n,
    input alufn[6],
    output cmp[8]
  ) {

  always {
    cmp[7:0] = 8h0;
    if (alufn[1] == 0 & alufn[0] == 0){
    cmp[0] = z;
      }
    if(alufn[1] == 1 & alufn[0] == 0){
    cmp[0] = n^v;
      }
    if(alufn[1] == 1 & alufn[0] == 1){
    cmp[0] =z|(n^v);}
   
}}
